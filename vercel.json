{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "python3.11",
      "maxDuration": 60,
      "memory": 1024,
      "excludeFiles": "**/{__pycache__,tests,knowledge,*.egg-info}/** *.md *.pyc *.pyo"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}
