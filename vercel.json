{
  "version": 2,
  "functions": {
    "api/main.py": {
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      },
      "excludeFiles": "**/{__pycache__,tests,knowledge,*.egg-info}/** *.md *.pyc *.pyo"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}
